            System.out.println("‚úÖ CobolPreprocessor loaded: " + preprocessorClass.getName());
            
            Class<?> programClass = Class.forName("io.github.uwol.proleap.cobol.asg.metamodel.Program");
            System.out.println("‚úÖ Program loaded: " + programClass.getName());
            
            Class<?> compilationUnitClass = Class.forName("io.github.uwol.proleap.cobol.asg.metamodel.CompilationUnit");
            System.out.println("‚úÖ CompilationUnit loaded: " + compilationUnitClass.getName());
            
            System.out.println("\\nüéâ All ProLeap classes loaded successfully!");
            
