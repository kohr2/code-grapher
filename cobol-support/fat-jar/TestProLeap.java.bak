// Test if we can load the ProLeap classes
public class TestProLeap {
    public static void main(String[] args) {
        try {
            System.out.println("Testing ProLeap class loading...");
            
            // Try to load the classes using reflection
            Class<?> runnerClass = Class.forName("io.github.uwol.proleap.cobol.asg.runner.impl.CobolParserRunnerImpl");
            System.out.println("‚úÖ CobolParserRunnerImpl loaded: " + runnerClass.getName());
            
